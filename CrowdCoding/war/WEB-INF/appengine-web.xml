<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>crowd-coding-dev2</application>
  <version>1</version>
  <threadsafe>true</threadsafe>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>

  <static-files>
    <exclude path="node_modules/**" ></exclude>
    <exclude path="bower_components/**" ></exclude>
    <exclude path="client/.tmp/**" ></exclude>
    <exclude path="admin/.tmp/**" ></exclude>
  </static-files>
  
  <resource-files>
    <exclude path="node_modules/**" ></exclude>
    <exclude path="bower_components/**" ></exclude>
    <exclude path="client/.tmp/**" ></exclude>
    <exclude path="admin/.tmp/**" ></exclude>
  </resource-files>
  
</appengine-web-app>
